,"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":43,"column":6}},{"generated":{"line":81,"column":12},"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":24,"column":4}},{"generated":{"line":81,"column":13},"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":23,"column":2}},{"generated":{"line":82,"column":0},"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":52,"column":1}},{"generated":{"line":82,"column":1},"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":6,"column":0}},{"name":"MessageBox","generated":{"line":84,"column":15},"source":"components/Chat/MessageBox/MessageBox.js","original":{"line":54,"column":15}}],"sources":{"components/Chat/MessageBox/MessageBox.js":"import React,{useState, useRef} from \"react\";\r\nimport TextField from \"@material-ui/core/TextField\";\r\nimport Button from \"@material-ui/core/Button\";\r\n\r\n//renaming prop for use in the component\r\nconst MessageBox = (props) => {\r\n  const [message, setMessage] = useState(\"\");\r\n  const messageRef = useRef(\"\");\r\n\r\n  const sendMessageClick = () =>{\r\n    if(messageRef.current.value === \"\"){\r\n      return false;\r\n    }\r\n    const messageObject = {\r\n      user_name: props.userData.currentUserData.user_name,\r\n      user_avatar: props.userData.currentUserData.user_avatar,\r\n      message: messageRef.current.value\r\n    }\r\n    props.onSendMessage(messageObject);\r\n    setMessage(\"\");\r\n  }\r\n\r\n  return (\r\n    <form className=\"chat-form\" autoComplete=\"off\">\r\n      <TextField\r\n        id=\"standard-basic\"\r\n        label=\"Write your message here\"\r\n        margin=\"normal\"\r\n        multiline\r\n        fullWidth\r\n        rows=\"4\"\r\n        inputRef={messageRef}\r\n        onChange={event => setMessage(event.target.value)}\r\n        onKeyDown={event => {\r\n          if(event.key === \"Enter\"){\r\n            //prevents enter from being pressed\r\n            event.preventDefault();\r\n            sendMessageClick();\r\n          }\r\n        }}\r\n        value={message}\r\n      />\r\n      <Button\r\n        variant=\"contained\"\r\n        color=\"primary\"\r\n        onClick={sendMessageClick}\r\n      >\r\n        Send\r\n      </Button>\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default MessageBox;"},"lineCount":null}},"error":null,"hash":"756fb35f49648e7df8e90b87aa16bf7f","cacheData":{"env":{}}}